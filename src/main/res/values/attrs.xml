<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AbstractDbEditText">
        <attr name="tableName" format="string" />
        <attr name="tableColumnName" format="string" />
        <attr name="valueType" format="enum">
            <enum name="text" value="0"/>
            <enum name="integer" value="1"/>
            <enum name="real" value="2"/>
        </attr>
        <attr name="defValueStr" format="string" />
        <attr name="minValueStr" format="string" />
        <attr name="maxValueStr" format="string" />
        <attr name="minFractionDigits" format="integer" />
        <attr name="maxFractionDigits" format="integer" />
        <attr name="hintTypeface" format="enum">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2"/>
            <enum name="boldItalic" value="3"/>
        </attr>
        <attr name="hintShowDefValue" format="boolean" />
    </declare-styleable>
</resources>